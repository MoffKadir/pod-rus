			datacontext = "[GetMapMode( 'realms' )]"
		}
		parentanchor = center
		size = { 30 30 }

		onclick = "[SetMapMode( MapMode.GetKey )]"
		alwaystransparent = "[IsMapMode( MapMode.GetKey )]"

		tooltip = "[MapMode.GetDesc]"
		using = tooltip_ws
	}

