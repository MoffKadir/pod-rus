						shortcut = "map_mode_3"
						# CfV POD: clear custom map modes
						onclick = "[GetScriptedGui('pod_reset_title_name').Execute(GuiScope.End)]"
						onclick = "[GetVariableSystem.Clear('pod_mapmode_gui_glow')]"
					}
				}

				icon_button_mapmode = {
					name = "map_mode_cultures_button"
					size = { 32 32 }
